      <th>LastName</th>
      <th>MobileNumber</th>
      <th>Status</th>
      <th>Role</th>
      <th>Actions</th>
    </tr>
<% @teachers.each do |teacher| %>
<tr>

<td><%= teacher.user.email %></td>

<td><%= teacher.first_name %></td>
<td><%= teacher.last_name %></td>
<td><%= teacher.mobile_number %></td>


<td><%= %></td>
      <td><%= teacher.user.role %></td>
      <td><%= link_to "Delete","#" %> </td>
      <td><%= link_to "Assign Class",assign_class_teacher_path(teacher) %> </td>
</tr>
<% end %>
</table>
